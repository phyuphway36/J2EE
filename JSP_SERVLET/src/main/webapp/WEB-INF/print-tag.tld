<taglib> 	
	<tlib-version>1.0</tlib-version>
	<jsp-version>3.0</jsp-version>
	<short-name>Print Tag</short-name>
	
	<tag> 
	<name>print</name>
	<tag-class>tag.printTag</tag-class>
	<body-content>scriptless</body-content>
	
	<attribute>
	<name>text</name>
	<required>true</required>
	
	</attribute>
	</tag>


</taglib>